    "http-hash": "^2.0.0",
    "in-publish": "^2.0.0",
    "lodash": "^4.13.1",
    "postcss-cssnext": "^2.7.0",
    "postcss-font-awesome": "^0.3.3",
    "react-intl": "^2.1.2",
    "zen-observable": "^0.2.1"
