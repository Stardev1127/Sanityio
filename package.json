{
  "name": "@sanity/base",
  "version": "0.2.9",
  "description": "Sanity plugin containing the base components and roles for a Sanity configuration",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf lib",
    "compile": "babel --copy-files -d lib/ src/",
    "compile:watch": "npm run compile -- --watch",
    "coverage": "nyc --report=lcov --report=text _mocha -- test/**/*.test.js",
    "postpublish": "npm run clean",
    "posttest": "eslint .",
    "prepublish": "in-publish && npm run compile && sanity-check || not-in-publish",
    "test": "mocha test/**/*.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanity-io/base.git"
  },
  "keywords": [
    "sanity",
    "base"
  ],
  "author": "Sanity.io <hello@sanity.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sanity-io/base/issues"
  },
  "homepage": "https://github.com/sanity-io/base#readme",
  "dependencies": {
    "@sanity/client": "^0.3.12",
    "@sanity/document-store": "0.1.0",
    "@sanity/observable": "^1.0.2",
    "@sanity/state-router": "^0.2.1",
    "debounce-promise": "^2.1.1",
    "history": "2",
    "http-hash": "^2.0.0",
    "in-publish": "^2.0.0",
    "lodash": "^4.17.2",
    "postcss-cssnext": "^2.8.0",
    "react-icons": "^2.2.1",
    "react-intl": "^2.1.5"
  },
  "devDependencies": {
    "@sanity/check": "^0.0.3",
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-lodash": "^3.2.10",
    "babel-plugin-syntax-class-properties": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "eslint": "^3.10.2",
    "eslint-config-sanity": "^1.1.4",
    "eslint-plugin-react": "^6.7.1",
    "mocha": "^3.1.2",
    "nyc": "^10.0.0",
    "proxyquire": "^1.7.10",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "rimraf": "^2.5.4"
  },
  "peerDependencies": {
    "react": ">=15.1.0",
    "react-dom": ">=15.1.0"
  }
}
