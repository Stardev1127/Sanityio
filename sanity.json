    },
    {
      "name": "tool:@sanity/base/tool",
      "description": "Tools available for use within a Sanity configuration"
    },
    {
      "name": "style:@sanity/base/theme",
      "description": "Base theme variables for Sanity"
    },
    {
      "name": "locale:@sanity/base/nb-NO",
      "description": "Norwegian Bokm√•l"
    },
    {
      "name": "locale:@sanity/base/en-US",
