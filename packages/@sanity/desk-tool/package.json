{
  "name": "@sanity/desk-tool",
  "version": "2.30.0",
  "description": "Tool for managing all sorts of content in a structured manner",
  "main": "./lib/_exports/index.cjs",
  "module": "./lib/_exports/index.js",
  "exports": {
    ".": {
      "source": "./src/_exports/index.ts",
      "require": "./lib/_exports/index.cjs",
      "default": "./lib/_exports/index.js"
    },
    "./_internal": {
      "source": "./src/_exports/_internal.ts",
      "require": "./lib/_exports/_internal.cjs",
      "default": "./lib/_exports/_internal.js"
    },
    "./components": {
      "source": "./src/_exports/components.ts",
      "require": "./lib/_exports/components.cjs",
      "default": "./lib/_exports/components.js"
    }
  },
  "types": "./lib/dts/src/_exports/index.d.ts",
  "//": "the typesVersion config below is a workaround for TypeScript's lack of support for package exports",
  "typesVersions": {
    "*": {
      "_internal": [
        "./lib/dts/src/_exports/_internal.d.ts"
      ],
      "components": [
        "./lib/dts/src/_exports/components.d.ts"
      ]
    }
  },
  "author": "Sanity.io <hello@sanity.io>",
  "license": "MIT",
  "scripts": {
    "build": "../../../bin/pkg-utils bundle",
    "clean": "rimraf lib",
    "exports:check": "../../../bin/pkg-utils exports-check",
    "exports:clean": "../../../bin/pkg-utils exports-clean",
    "exports:generate": "../../../bin/pkg-utils exports-generate",
    "postbuild": "run-s exports:generate",
    "lint": "eslint . --ext .js,.jsx,.mjs,.ts,.tsx --quiet",
    "test": "jest",
    "watch": "../../../bin/pkg-utils bundle --watch"
  },
  "keywords": [
    "sanity",
    "cms",
    "headless",
    "realtime",
    "content",
    "desk-tool",
    "sanity-plugin",
    "sanity-tool"
  ],
  "dependencies": {
    "@reach/auto-id": "^0.13.2",
    "@sanity/client": "^3.2.0",
    "@sanity/data-aspects": "2.29.3",
    "@sanity/form-builder": "2.30.0",
    "@sanity/generate-help-url": "^3.0.0",
    "@sanity/icons": "^1.2.6",
    "@sanity/schema": "2.29.3",
    "@sanity/types": "2.29.5",
    "@sanity/ui": "^0.37.9",
    "@sanity/util": "2.29.5",
    "@sanity/uuid": "^3.0.1",
    "framer-motion": "^5.3.3",
    "hashlru": "^2.1.0",
    "is-hotkey": "^0.1.6",
    "leven": "^3.1.0",
    "lodash": "^4.17.21",
    "nanoid": "^3.1.30",
    "react-copy-to-clipboard": "^5.0.4",
    "react-is": "^17.0.2",
    "react-json-inspector": "^7.1.1",
    "react-rx": "^1.0.0-beta.6",
    "rxjs": "^6.5.3",
    "shallow-equals": "^1.0.0"
  },
  "devDependencies": {
    "@sanity/ui-workshop": "^0.4.1",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^11.2.5",
    "@types/is-hotkey": "^0.1.3",
    "@types/react-copy-to-clipboard": "^5.0.2",
    "@types/shallow-equals": "^1.0.0",
    "@types/webpack-env": "^1.16.2",
    "react": "17.0.2",
    "rimraf": "^3.0.2",
    "styled-components": "^5.2.0"
  },
  "peerDependencies": {
    "@sanity/base": "^2.0",
    "react": "^16.9 || ^17",
    "styled-components": "^5.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanity-io/sanity.git",
    "directory": "packages/@sanity/desk-tool"
  },
  "bugs": {
    "url": "https://github.com/sanity-io/sanity/issues"
  },
  "homepage": "https://www.sanity.io/"
}
