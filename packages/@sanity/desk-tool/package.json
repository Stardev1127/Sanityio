{
  "name": "@sanity/desk-tool",
  "version": "0.3.24",
  "description": "Tool for managing all sorts of content in a structured manner",
  "main": "src/plugin.js",
  "author": "Sanity.io <hello@sanity.io>",
  "license": "MIT",
  "scripts": {
    "example-testbed": "cd examples/testbed && sanity start",
    "start": "npm run example-testbed",
    "clean": "rimraf lib",
    "test": "sanity-check",
    "lint": "eslint --quiet .",
    "compile": "babel --out-dir lib/ src/ --copy-files",
    "compile:watch": "npm run compile -- --watch",
    "postpublish": "npm run clean",
    "posttest": "eslint --quiet .",
    "prepublish": "in-publish && npm run compile && sanity-check || not-in-publish"
  },
  "keywords": [
    "sanity",
    "sanity-plugin",
    "sanity-tool"
  ],
  "dependencies": {
    "@sanity/data-aspects": "^0.0.5",
    "@sanity/form-builder": "^0.1.20",
    "react-click-outside": "^2.2.0"
  },
  "devDependencies": {
    "@sanity/base": "^0.3.12",
    "@sanity/check": "^0.0.3",
    "@sanity/client": "^0.4.8",
    "@sanity/components": "^0.0.94",
    "@sanity/core": "^0.0.36",
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-syntax-class-properties": "^6.13.0",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "eslint": "^3.12.2",
    "eslint-config-sanity": "^2.0.1",
    "eslint-plugin-react": "^6.8.0",
    "history": "^4.5.0",
    "in-publish": "^2.0.0",
    "postcss-cssnext": "^2.9.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "rimraf": "^2.5.4"
  },
  "peerDependencies": {
    "react": "^15.3.0",
    "react-dom": "^15.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:sanity-io/desk-tool.git"
  }
}
