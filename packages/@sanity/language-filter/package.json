{
  "name": "@sanity/language-filter",
  "version": "2.29.8",
  "description": "A Sanity plugin that supports filtering localized fields by language",
  "main": "./lib/filter-fields.cjs",
  "module": "./lib/filter-fields.js",
  "exports": {
    ".": {
      "source": "./src/filter-fields.js",
      "require": "./lib/filter-fields.cjs",
      "default": "./lib/filter-fields.js"
    },
    "./SelectLanguageProvider": {
      "source": "./src/SelectLanguageProvider.tsx",
      "require": "./lib/SelectLanguageProvider.cjs",
      "default": "./lib/SelectLanguageProvider.js"
    }
  },
  "types": "./lib/dts/src/filter-fields.js",
  "scripts": {
    "build": "../../../bin/pkg-utils bundle",
    "clean": "rimraf lib",
    "_exports:check": "../../../bin/pkg-utils exports-check",
    "exports:clean": "../../../bin/pkg-utils exports-clean",
    "exports:generate": "../../../bin/pkg-utils exports-generate",
    "postbuild": "run-s exports:generate",
    "prebuild": "yarn clean",
    "watch": "../../../bin/pkg-utils bundle --watch"
  },
  "keywords": [
    "sanity",
    "cms",
    "headless",
    "realtime",
    "content",
    "language-filter"
  ],
  "dependencies": {
    "@sanity/icons": "^1.2.6",
    "@sanity/types": "2.29.5",
    "@sanity/ui": "^0.37.9",
    "lodash": "^4.17.21",
    "rxjs": "^6.5.3"
  },
  "devDependencies": {
    "prop-types": "^15.6.0",
    "react": "17.0.2"
  },
  "peerDependencies": {
    "prop-types": "^15.3",
    "react": "^16.9 || ^17"
  },
  "author": "Sanity.io <hello@sanity.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sanity-io/sanity/issues"
  },
  "homepage": "https://www.sanity.io/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanity-io/sanity.git",
    "directory": "packages/@sanity/language-filter"
  },
  "publishConfig": {
    "access": "public"
  }
}
