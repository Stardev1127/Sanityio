{
  "name": "@sanity/server",
  "version": "1.3.1",
  "description": "Webserver dedicated to serving Sanity, allowing a smooth development experience",
  "main": "lib/server.js",
  "scripts": {
    "clean": "rimraf lib",
    "compile": "babel -d lib/ src/",
    "compile:watch": "babel -w -d lib/ src/",
    "coverage": "babel-node ./node_modules/.bin/isparta cover --report=lcov --report=text _mocha -- test/**/*.test.js",
    "posttest": "eslint .",
    "prepublish": "in-publish && npm run compile || not-in-publish",
    "postpublish": "in-publish && npm run clean || not-in-publish",
    "start": "node bin/sanity-server.js",
    "start-dev": "node bin/sanity-server.js --dev",
    "test": "mocha test/**/*.test.js"
  },
  "bin": {
    "sanity-server": "./bin/sanity-server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/sanity-io/server.git"
  },
  "keywords": [
    "sanity",
    "webserver"
  ],
  "author": "Sanity.io <hello@sanity.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sanity-io/server/issues"
  },
  "homepage": "https://github.com/sanity-io/server#readme",
  "dependencies": {
    "@sanity/resolver": "^3.0.0",
    "@sanity/webpack-loader": "^2.0.1",
    "babel-core": "^6.5.2",
    "babel-loader": "^6.2.2",
    "babel-plugin-syntax-class-properties": "^6.8.0",
    "babel-plugin-transform-class-properties": "^6.9.1",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.0",
    "babel-register": "^6.5.1",
    "css-loader": "^0.25.0",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "in-publish": "^2.0.0",
    "json-loader": "^0.5.4",
    "lodash": "^4.5.0",
    "parents": "^1.0.1",
    "postcss-cssnext": "^2.8.0",
    "postcss-import": "^8.1.0",
    "postcss-loader": "^0.13.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "require-uncached": "^1.0.2",
    "resolve": "^1.1.7",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.7.0",
    "webpack-hot-middleware": "^2.12.2"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-eslint": "^6.0.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.2.0",
    "cross-env": "^1.0.7",
    "eslint": "^2.5.3",
    "eslint-config-sanity": "^1.0.7",
    "eslint-plugin-react": "^4.2.3",
    "isparta": "^4.0.0",
    "mocha": "^2.4.5",
    "rimraf": "^2.5.2"
  },
  "peerDependencies": {
    "@sanity/base": ">=2.0.0",
    "react": "^15.0.0",
    "react-dom": "^15.0.0"
  }
}
