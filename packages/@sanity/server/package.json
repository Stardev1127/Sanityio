{
  "name": "@sanity/server",
  "version": "0.0.8",
  "description": "Webserver dedicated to serving Sanity, allowing a smooth development experience",
  "main": "lib/server.js",
  "scripts": {
    "clean": "rimraf lib",
    "compile": "babel -d lib/ src/ && cp -r src/templates lib/",
    "coverage": "babel-node ./node_modules/.bin/isparta cover --report=lcov --report=text _mocha -- test/**/*.test.js",
    "posttest": "eslint .",
    "prepublish": "in-publish && npm run compile || not-in-publish",
    "start": "node bin/sanity-server.js",
    "start-dev": "node bin/sanity-server.js --dev",
    "test": "mocha test/**/*.test.js"
  },
  "bin": {
    "sanity-server": "./bin/sanity-server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/sanity-io/server.git"
  },
  "keywords": [
    "sanity",
    "webserver"
  ],
  "author": "Sanity.io <hello@sanity.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sanity-io/server/issues"
  },
  "homepage": "https://github.com/sanity-io/server#readme",
  "dependencies": {
    "@sanity/plugin-loader": "0.0.8",
    "babel-core": "^6.5.2",
    "babel-eslint": "^5.0.0-beta4",
    "babel-loader": "^6.2.2",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.0",
    "configstore": "^1.4.0",
    "debug": "^2.2.0",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.13.4",
    "in-publish": "^2.0.0",
    "lodash": "^4.5.0",
    "webpack": "^1.12.14",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-hot-middleware": "^2.9.1"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-register": "^6.5.1",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.2.0",
    "cross-env": "^1.0.7",
    "eslint": "^1.10.3",
    "eslint-config-bengler": "^0.5.6",
    "eslint-plugin-react": "^3.16.1",
    "isparta": "^4.0.0",
    "mocha": "^2.4.5",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "rimraf": "^2.5.2"
  },
  "peerDependencies": {
    "@sanity/base": "*",
    "react": ">=0.14.0",
    "react-dom": ">=0.14.0"
  }
}
