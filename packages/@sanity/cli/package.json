{
  "name": "@sanity/cli",
  "version": "0.0.27",
  "description": "Sanity CLI tool for managing Sanity installations, managing plugins, schemas and datasets",
  "main": "lib/cli.js",
  "bin": {
    "sanity": "./bin/run.js"
  },
  "scripts": {
    "compile": "babel --source-maps inline --copy-files -d lib/ src/",
    "compile:watch": "babel --source-maps inline --watch --copy-files -d lib/ src/",
    "test": "mocha test/**/*.test.js",
    "clean": "rimraf lib",
    "prepublish": "in-publish && npm run compile || not-in-publish",
    "postpublish": "npm run clean",
    "posttest": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/sanity-io/cli.git"
  },
  "keywords": [
    "sanity",
    "cli",
    "tool"
  ],
  "author": "Sanity.io <hello@sanity.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sanity-io/cli/issues"
  },
  "homepage": "https://github.com/sanity-io/cli#readme",
  "dependencies": {
    "@sanity/client": "^0.3.2",
    "@sanity/resolver": "^0.0.4",
    "babel-polyfill": "^6.13.0",
    "chalk": "^1.1.3",
    "configstore": "^2.0.0",
    "debug": "^2.2.0",
    "deep-sort-object": "^1.0.1",
    "execa": "^0.5.0",
    "fs-promise": "^0.5.0",
    "git-user-info": "^1.0.0",
    "gitconfiglocal": "^2.0.1",
    "github-url-to-object": "^2.1.0",
    "got": "^6.3.0",
    "in-publish": "^2.0.0",
    "inquirer": "^1.2.2",
    "is-git-url": "^0.2.3",
    "latest-version": "^2.0.0",
    "lodash": "^4.5.0",
    "minimist": "^1.2.0",
    "opn": "^4.0.2",
    "ora": "^0.3.0",
    "progrescii": "^0.1.1",
    "promise-props-recursive": "^1.0.0",
    "resolve-from": "^2.0.0",
    "split2": "^2.1.0",
    "thenify": "^3.2.0",
    "update-notifier": "^1.0.2",
    "validate-npm-package-name": "^2.2.2",
    "which": "^1.2.11",
    "yarn": "^0.16.1"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-eslint": "^7.0.0",
    "babel-preset-es2015-node4": "^2.1.0",
    "babel-preset-stage-2": "^6.13.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "eslint": "^3.8.1",
    "eslint-config-sanity": "^1.1.3",
    "mocha": "^3.1.2",
    "rimraf": "^2.5.2",
    "source-map-support": "^0.4.6"
  }
}
