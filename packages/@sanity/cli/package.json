    "babel-preset-stage-2": "^6.13.0",
    "babel-register": "^6.9.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "eslint": "^1.10.3",
    "eslint-config-bengler": "^0.5.6",
    "mocha": "^2.4.5",
    "rimraf": "^2.5.2"
  }
}
