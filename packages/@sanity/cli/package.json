{
  "name": "@sanity/cli",
  "version": "1.0.6",
  "description": "Sanity CLI tool for managing Sanity installations, managing plugins, schemas and datasets",
  "main": "lib/cli.js",
  "bin": {
    "sanity": "./bin/run.js"
  },
  "scripts": {
    "compile": "babel -d lib/ src/",
    "coverage": "babel-node ./node_modules/.bin/isparta cover --report=lcov --report=text _mocha -- test/**/*.test.js",
    "test": "mocha test/**/*.test.js",
    "clean": "rimraf lib",
    "prepublish": "in-publish && npm run compile || not-in-publish",
    "postpublish": "npm run clean",
    "posttest": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/sanity-io/cli.git"
  },
  "keywords": [
    "sanity",
    "cli",
    "tool"
  ],
  "author": "Sanity.io <hello@sanity.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sanity-io/cli/issues"
  },
  "homepage": "https://github.com/sanity-io/cli#readme",
  "dependencies": {
    "@sanity/server": "^1.1.6",
    "child-process-promise": "^1.1.0",
    "configstore": "^1.4.0",
    "debug": "^2.2.0",
    "deep-sort-object": "^1.0.1",
    "fs-extra": "^0.26.5",
    "fs-promise": "^0.4.1",
    "git-user-info": "^1.0.0",
    "gitconfiglocal": "^2.0.1",
    "github-url-to-object": "^2.1.0",
    "in-publish": "^2.0.0",
    "inquirer": "^0.12.0",
    "is-git-url": "^0.2.3",
    "joi": "^7.3.0",
    "lodash": "^4.5.0",
    "npm": "^3.7.2",
    "ora": "^0.2.0",
    "promise-props-recursive": "^1.0.0",
    "semver-compare": "^1.0.0",
    "thenify": "^3.2.0",
    "validate-npm-package-name": "^2.2.2",
    "yargs": "^3.32.0"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-eslint": "^4.1.8",
    "babel-preset-es2015": "^6.5.0",
    "babel-register": "^6.5.1",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.2.0",
    "eslint": "^1.10.3",
    "eslint-config-bengler": "^0.5.6",
    "isparta": "^4.0.0",
    "mocha": "^2.4.5",
    "rimraf": "^2.5.2",
    "which": "^1.2.4"
  }
}
