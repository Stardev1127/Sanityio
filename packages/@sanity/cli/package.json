{
  "name": "@sanity/cli",
  "version": "0.0.5",
  "description": "Sanity CLI tool for managing Sanity installations, managing plugins, schemas and datasets",
  "main": "lib/cli.js",
  "bin": {
    "sanity": "./bin/run.js"
  },
  "scripts": {
    "compile": "babel --copy-files -d lib/ src/",
    "compile:watch": "babel --watch --copy-files -d lib/ src/",
    "test": "mocha test/**/*.test.js",
    "clean": "rimraf lib",
    "prepublish": "in-publish && npm run compile || not-in-publish",
    "postpublish": "npm run clean",
    "posttest": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/sanity-io/cli.git"
  },
  "keywords": [
    "sanity",
    "cli",
    "tool"
  ],
  "author": "Sanity.io <hello@sanity.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sanity-io/cli/issues"
  },
  "homepage": "https://github.com/sanity-io/cli#readme",
  "dependencies": {
    "@sanity/check": "^0.0.1",
    "@sanity/client": "^0.0.1",
    "@sanity/resolver": "^0.0.1",
    "@sanity/server": "^0.0.2",
    "@sanity/storybook": "0.0.6",
    "babel-polyfill": "^6.13.0",
    "chalk": "^1.1.3",
    "child-process-promise": "^1.1.0",
    "configstore": "^2.0.0",
    "debug": "^2.2.0",
    "deep-sort-object": "^1.0.1",
    "filesize": "^3.3.0",
    "fs-extra": "^0.26.5",
    "fs-promise": "^0.5.0",
    "git-user-info": "^1.0.0",
    "gitconfiglocal": "^2.0.1",
    "github-url-to-object": "^2.1.0",
    "got": "^6.3.0",
    "in-publish": "^2.0.0",
    "inquirer": "^1.1.2",
    "is-git-url": "^0.2.3",
    "lodash": "^4.5.0",
    "npm": "^3.7.2",
    "opn": "^4.0.2",
    "ora": "^0.3.0",
    "path-exists": "^3.0.0",
    "promise-props-recursive": "^1.0.0",
    "semver-compare": "^1.0.0",
    "thenify": "^3.2.0",
    "update-notifier": "^1.0.2",
    "validate-npm-package-name": "^2.2.2",
    "which": "^1.2.4",
    "yargs": "^5.0.0"
