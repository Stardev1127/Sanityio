{
  "name": "@sanity/core",
  "version": "0.0.6",
  "description": "Sanity core bundle, containing required packages for the development and build process",
  "main": "lib/index.js",
  "author": "Sanity.io <hello@sanity.io>",
  "license": "MIT",
  "scripts": {
    "compile": "babel --source-maps inline --copy-files -d lib/ src/",
    "compile:watch": "babel --source-maps inline --watch --copy-files -d lib/ src/",
    "prepublish": "in-publish && npm run compile || not-in-publish",
    "postpublish": "rimraf lib",
    "test": "eslint ."
  },
  "keywords": [
    "sanity"
  ],
  "dependencies": {
    "@sanity/check": "^0.0.2",
    "@sanity/server": "^0.0.13",
    "@sanity/storybook": "^0.0.9",
    "@sanity/util": "0.0.2",
    "deep-sort-object": "^1.0.1",
    "filesize": "^3.3.0",
    "in-publish": "^2.0.0",
    "path-exists": "^3.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-eslint": "^7.1.0",
    "babel-preset-es2015-node4": "^2.1.0",
    "babel-preset-stage-2": "^6.18.0",
    "eslint": "^3.4.0",
    "eslint-config-sanity": "^1.1.3",
    "rimraf": "^2.5.2"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:sanity-io/core.git"
  }
}
