{
  "name": "@sanity/default-layout",
  "version": "0.1.2",
  "description": "The default layout components for Sanity",
  "main": "lib/default-layout.js",
  "scripts": {
    "start": "cd example && sanity start",
    "compile": "babel --copy-files -d lib/ src/",
    "compile:watch": "babel --source-maps inline --watch --copy-files -d lib/ src/",
    "coverage": "nyc --report=lcov --report=text _mocha -- test/**/*.test.js",
    "test": "eslint .",
    "clean": "rimraf lib .nyc_output coverage",
    "prepublish": "in-publish && npm run compile && sanity-check || not-in-publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanity-io/default-layout.git"
  },
  "keywords": [
    "sanity",
    "layout"
  ],
  "author": "Sanity.io <hello@sanity.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sanity-io/default-layout/issues"
  },
  "homepage": "https://github.com/sanity-io/default-layout#readme",
  "dependencies": {
  },
  "devDependencies": {
    "@sanity/base": "^0.2.0",
    "@sanity/check": "^0.0.3",
    "@sanity/cli": "^0.1.13",
    "@sanity/components": "^0.0.59",
    "@sanity/core": "^0.0.13",
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-syntax-class-properties": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "eslint": "^3.10.2",
    "eslint-config-sanity": "^1.1.4",
    "eslint-plugin-react": "^6.7.1",
    "in-publish": "^2.0.0",
    "mocha": "^3.1.2",
    "nyc": "^10.0.0",
    "react": "^15.4.1",
    "rimraf": "^2.5.4"
  },
  "peerDependencies": {
    "react": "^15.4.1",
    "react-dom": "^15.3.2"
  }
}
