{
  "name": "@sanity/client-next",
  "version": "2.0.13",
  "description": "Client for retrieving data from Sanity",
  "main": "lib/sanityClient.js",
  "umd": "umd/sanityClient.min.js",
  "scripts": {
    "browserify": "NODE_ENV=production DEBUG='' browserify -t envify -g uglifyify lib/sanityClient.js -o umd/sanityClient.js --standalone=SanityClient",
    "build": "npm run clean && npm run compile && npm run browserify && npm run minify",
    "clean": "rimraf lib coverage .nyc_output umd/*.js",
    "compile": "babel --source-maps --copy-files -d lib/ src/",
    "coverage": "DEBUG=sanity nyc --reporter=html faucet",
    "minify": "uglifyjs -c -m -- umd/sanityClient.js > umd/sanityClient.min.js",
    "postpublish": "npm run clean",
    "posttest": "eslint .",
    "prepublish": "in-publish && npm run build || not-in-publish",
    "test": "faucet"
  },
  "dependencies": {
    "@sanity/request": "^1.0.1",
    "debug": "^2.2.0",
    "deep-assign": "^2.0.0",
    "in-publish": "^2.0.0",
    "xtend": "^4.0.1"
  },
  "browser": {
    "request": "./lib/browser/request.js"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-preset-es2015": "^6.14.0",
    "browserify": "^13.1.0",
    "envify": "^3.4.1",
    "eslint": "^2.7.0",
    "eslint-config-sanity": "^1.0.7",
    "faucet": "0.0.1",
    "nock": "^8.0.0",
    "nyc": "^6.4.0",
    "rimraf": "^2.5.4",
    "tape": "^4.6.0",
    "uglify-js": "^2.7.3",
    "uglifyify": "^3.0.2"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/sanity-io/client.git"
  },
  "keywords": [
    "sanity",
    "client",
    "fetch",
    "api",
    "gradient"
  ],
  "author": "Sanity.io <hello@sanity.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sanity-io/client/issues"
  },
  "homepage": "https://github.com/sanity-io/client#readme"
}
