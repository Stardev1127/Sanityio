{
  "name": "@sanity/client-next",
  "version": "1.1.7",
  "description": "Client for retrieving data from Sanity",
  "main": "lib/client.cjs.js",
  "umd": "umd/bundle.min.js",
  "scripts": {
    "build": "npm run compile && npm run minify",
    "clean": "rimraf lib && rimraf coverage",
    "compile": "rollup -c",
    "coverage": "nyc --reporter=html ava",
    "minify": "uglifyjs --compress --mangle -- umd/bundle.js > umd/bundle.min.js",
    "postpublish": "npm run clean",
    "posttest": "eslint .",
    "prepublish": "in-publish && npm run compile || not-in-publish",
    "test": "ava"
  },
  "dependencies": {
    "@sanity/request": "^1.0.1",
    "in-publish": "^2.0.0",
    "object-assign": "^4.1.0"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-eslint": "^6.1.2",
    "babel-preset-es2015": "^6.14.0",
    "babel-register": "^6.14.0",
    "debug": "^2.2.0",
    "eslint": "^2.7.0",
    "eslint-config-sanity": "^1.0.7",
    "nock": "^8.0.0",
    "nyc": "^6.4.0",
    "rimraf": "^2.5.4",
    "rollup": "^0.34.13",
    "rollup-plugin-buble": "^0.13.0",
    "uglify-js": "^2.7.3"
  },
  "ava": {
    "babel": "inherit",
    "require": ["babel-register"]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/sanity-io/client.git"
  },
  "keywords": [
    "sanity",
    "client",
    "fetch",
    "api",
    "gradient"
  ],
  "author": "Sanity.io <hello@sanity.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sanity-io/client/issues"
  },
  "homepage": "https://github.com/sanity-io/client#readme"
}
