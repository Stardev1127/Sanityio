{
  "name": "@sanity/client-next",
  "version": "1.1.8",
  "description": "Client for retrieving data from Sanity",
  "main": "lib/client.js",
  "umd": "umd/bundle.min.js",
  "scripts": {
    "browserify": "NODE_ENV=production DEBUG='' browserify -t envify -g uglifyify lib/client.js -o umd/bundle.js --standalone=SanityClient",
    "build": "npm run compile && npm run browserify",
    "clean": "rimraf lib coverage .nyc_output",
    "compile": "rimraf lib &&  babel --source-maps --copy-files -d lib/ src/",
    "coverage": "nyc --reporter=html tape -- -r babel-register test/**/*.test.js",
    "postpublish": "npm run clean",
    "posttest": "eslint .",
    "prepublish": "in-publish && npm run compile || not-in-publish",
    "test": "tape -r babel-register test/**/*.test.js"
  },
  "dependencies": {
    "deep-assign": "^2.0.0",
    "in-publish": "^2.0.0",
    "request": "^2.74.0",
    "xtend": "^4.0.1"
  },
  "browser": {
    "request": "./lib/browser/request.js"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-plugin-istanbul": "^2.0.1",
    "babel-preset-babili": "0.0.2",
    "babel-preset-es2015": "^6.14.0",
    "babel-register": "^6.14.0",
    "browserify": "^13.1.0",
    "debug": "^2.2.0",
    "envify": "^3.4.1",
    "eslint": "^2.7.0",
    "eslint-config-sanity": "^1.0.7",
    "nock": "^8.0.0",
    "nyc": "^6.4.0",
    "rimraf": "^2.5.4",
    "tape": "^4.6.0",
    "uglify-js": "^2.7.3",
    "uglifyify": "^3.0.2"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/sanity-io/client.git"
  },
  "keywords": [
    "sanity",
    "client",
    "fetch",
    "api",
    "gradient"
  ],
  "author": "Sanity.io <hello@sanity.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sanity-io/client/issues"
  },
  "homepage": "https://github.com/sanity-io/client#readme"
}
