{
  "name": "@sanity/imagetool",
  "version": "2.29.8",
  "description": "The image hotspot selector / cropper used in Sanity",
  "source": "./src/_exports/index.ts",
  "main": "./lib/cjs/_exports/index.js",
  "module": "./lib/esm/_exports/index.js",
  "exports": {
    ".": {
      "source": "./src/_exports/index.ts",
      "require": "./lib/cjs/_exports/index.js",
      "default": "./lib/esm/_exports/index.js"
    },
    "./calculateStyles": {
      "source": "./src/_exports/calculateStyles.ts",
      "require": "./lib/cjs/_exports/calculateStyles.js",
      "default": "./lib/esm/_exports/calculateStyles.js"
    },
    "./constants": {
      "source": "./src/_exports/constants.ts",
      "require": "./lib/cjs/_exports/constants.js",
      "default": "./lib/esm/_exports/constants.js"
    },
    "./HotspotImage": {
      "source": "./src/_exports/HotspotImage.ts",
      "require": "./lib/cjs/_exports/HotspotImage.js",
      "default": "./lib/esm/_exports/HotspotImage.js"
    }
  },
  "types": "./lib/dts/src/_exports/index.d.ts",
  "//": "the typesVersion config below is a workaround for TypeScript's lack of support for package exports",
  "typesVersions": {
    "*": {
      "calculateStyles": [
        "./lib/dts/src/_exports/calculateStyles.d.ts"
      ],
      "constants": [
        "./lib/dts/src/_exports/constants.d.ts"
      ],
      "HotspotImage": [
        "./lib/dts/src/_exports/HotspotImage.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "../../../bin/pkg-utils transpile",
    "clean": "rimraf lib",
    "exports:check": "../../../bin/pkg-utils exports-check",
    "exports:clean": "../../../bin/pkg-utils exports-clean",
    "exports:generate": "../../../bin/pkg-utils exports-generate",
    "postbuild": "run-s exports:generate",
    "start": "remon --require babel-core/register ./example/app.js",
    "test": "jest",
    "watch": "../../../bin/pkg-utils transpile --watch"
  },
  "author": "Sanity.io <hello@sanity.io>",
  "license": "MIT",
  "dependencies": {
    "debug": "^3.2.7",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "prop-types": "^15.6.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "rimraf": "^3.0.2",
    "styled-components": "^5.2.0",
    "ts-node": "^9.1.1"
  },
  "peerDependencies": {
    "prop-types": "^15.6 || ^16",
    "react": "^16.9 || ^17",
    "react-dom": "^16.9 || ^17",
    "styled-components": "^5.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanity-io/sanity.git",
    "directory": "packages/@sanity/imagetool"
  },
  "bugs": {
    "url": "https://github.com/sanity-io/sanity/issues"
  },
  "homepage": "https://www.sanity.io/",
  "directories": {
    "example": "example",
    "test": "test"
  },
  "keywords": [
    "sanity",
    "cms",
    "headless",
    "realtime",
    "content",
    "imagetool"
  ]
}
