{
  "name": "@sanity-legacy/imagetool",
  "version": "2.0.0",
  "description": "The image hotspot selector / cropper used in Sanity",
  "main": "index.js",
  "scripts": {
    "bloat": "browserify --extension=.jsx -t babelify --full-paths ./src/index.js | discify --open",
    "build": "babel src --out-dir lib --copy-files",
    "lint": "eslint .",
    "pre-commit-check": "npm test && (eslint . || (echo 'Warning: project has lint errors. Please fix and re-commit with `git commit --amend`' && echo))",
    "prepublish": "npm run build",
    "start": "remon --require babel/register ./example/app.js",
    "test": "mocha --recursive --require babel/register test",
    "watch": "npm run build -- --watch"
  },
  "author": "Bjørge Næss",
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.1.3",
    "debug": "^2.2.0",
    "dom-event": "0.0.5",
    "react": "0.14.x",
    "react-addons-pure-render-mixin": "0.14.x",
    "react-dom": "0.14.x"
  },
  "devDependencies": {
    "babel": "^5.4.7",
    "babelify": "^6.1.1",
    "browserify": "^10.2.1",
    "disc": "^1.3.2",
    "envify": "^3.4.0",
    "error-capture-middleware": "0.0.2",
    "eslint": "^1.0.0",
    "eslint-config-bengler": "^0.2.0",
    "eslint-plugin-react": "^3.2.0",
    "express": "^4.12.3",
    "mocha": "^2.2.4",
    "node-sass": "^3.4.2",
    "quickreload": "^1.0.6",
    "rebundler": "^0.1.0",
    "remon": "^1.0.1",
    "should": "^6.0.1",
    "staticr": "^2.1.2"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/bengler/sanity-imagetool.git"
  }
}
