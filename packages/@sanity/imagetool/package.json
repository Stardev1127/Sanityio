{
  "name": "@sanity/imagetool",
  "version": "1.1.7",
  "description": "The image hotspot selector / cropper used in Sanity",
  "main": "index.js",
  "scripts": {
    "start": "proxyreload ./example/app.js",
    "test": "mocha --recursive --require babel-core/register test",
    "lint": "eslint .",
    "pre-commit-check": "npm test && (eslint . || (echo 'Warning: project has lint errors. Please fix and re-commit with `git commit --amend`' && echo))",
    "watch": "npm run build -- --watch",
    "build": "babel src --out-dir lib",
    "prepublish": "npm run build"
  },
  "author": "Bjørge Næss",
  "license": "MIT",
  "dependencies": {
    "react": ">=0.13.0 <0.14.0",
    "rx": "^2.3.23"
  },
  "devDependencies": {
    "babel-core": "^5.2.9",
    "babelify": "^6.0.2",
    "browserify": "^9.0.8",
    "error-capture-middleware": "0.0.2",
    "eslint": "^0.21.2",
    "eslint-plugin-react": "^2.3.0",
    "express": "^4.12.3",
    "mocha": "^2.2.4",
    "node-sass": "^2.1.1",
    "proxyreload": "git://github.com/bjoerge/proxyreload",
    "quickreload": "^1.0.0-alpha.3",
    "rebundler": "0.0.4",
    "rx-dom": "^5.0.3",
    "should": "^6.0.1",
    "staticr": "^2.1.2"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "stage": 0
        }
      ]
    ]
  }
}
