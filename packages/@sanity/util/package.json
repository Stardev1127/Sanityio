{
  "name": "@sanity/util",
  "version": "3.0.0-dev-preview.20",
  "description": "Utilities shared across projects of Sanity",
  "types": "./lib/exports/index.d.ts",
  "source": "./exports/index.ts",
  "module": "./lib/index.mjs",
  "main": "./lib/index.js",
  "exports": {
    ".": {
      "types": "./lib/exports/index.d.ts",
      "source": "./exports/index.ts",
      "import": "./lib/index.mjs",
      "require": "./lib/index.js",
      "default": "./lib/index.mjs"
    },
    "./fs": {
      "types": "./lib/exports/fs.d.ts",
      "source": "./exports/fs.ts",
      "import": "./lib/fs.mjs",
      "require": "./lib/fs.js",
      "default": "./lib/fs.mjs"
    },
    "./content": {
      "types": "./lib/exports/content.d.ts",
      "source": "./exports/content.ts",
      "import": "./lib/content.mjs",
      "require": "./lib/content.js",
      "default": "./lib/content.mjs"
    },
    "./legacyDateFormat": {
      "types": "./lib/exports/legacyDateFormat.d.ts",
      "source": "./exports/legacyDateFormat.ts",
      "import": "./lib/legacyDateFormat.mjs",
      "require": "./lib/legacyDateFormat.js",
      "default": "./lib/legacyDateFormat.mjs"
    },
    "./paths": {
      "types": "./lib/exports/paths.d.ts",
      "source": "./exports/paths.ts",
      "import": "./lib/paths.mjs",
      "require": "./lib/paths.js",
      "default": "./lib/paths.mjs"
    }
  },
  "typesVersions": {
    "*": {
      "fs": [
        "./lib/exports/fs.d.ts"
      ],
      "content": [
        "./lib/exports/content.d.ts"
      ],
      "legacyDateFormat": [
        "./lib/exports/legacyDateFormat.d.ts"
      ],
      "paths": [
        "./lib/exports/paths.d.ts"
      ]
    }
  },
  "files": [
    "content.js",
    "fs.js",
    "legacyDateFormat.js",
    "lib",
    "paths.js",
    "src"
  ],
  "scripts": {
    "build": "pkg-utils build --tsconfig tsconfig.lib.json",
    "check:package": "pkg-utils --strict --tsconfig tsconfig.lib.json",
    "clean": "rimraf content.js fs.js legacyDateFormat.js lib paths.js",
    "test": "jest",
    "postbuild": "run-s check:package",
    "prebuild": "run-s clean",
    "watch": "pkg-utils watch --tsconfig tsconfig.lib.json"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanity-io/sanity.git",
    "directory": "packages/@sanity/util"
  },
  "keywords": [
    "sanity",
    "cms",
    "headless",
    "realtime",
    "content",
    "util"
  ],
  "author": "Sanity.io <hello@sanity.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sanity-io/sanity/issues"
  },
  "homepage": "https://www.sanity.io/",
  "dependencies": {
    "@sanity/types": "3.0.0-dev-preview.20",
    "get-random-values-esm": "^1.0.0",
    "moment": "^2.29.4"
  },
  "devDependencies": {
    "@types/moment": "^2.13.0",
    "rimraf": "^3.0.2"
  }
}
