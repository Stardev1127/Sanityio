{
  "name": "@sanity/util",
  "version": "2.29.5",
  "description": "Utilities shared across projects of Sanity",
  "source": "src/index.ts",
  "main": "lib/index.cjs",
  "module": "lib/index.js",
  "types": "lib/dts/index.d.ts",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "require": "./lib/index.cjs",
      "default": "./lib/index.js"
    },
    "./_internal": {
      "source": "./src/_internal.ts",
      "require": "./lib/_internal.cjs",
      "default": "./lib/_internal.js"
    },
    "./content": {
      "source": "./src/content/index.ts",
      "require": "./lib/content.cjs",
      "default": "./lib/content.js"
    },
    "./legacyDateFormat": {
      "source": "./src/legacyDateFormat.ts",
      "require": "./lib/legacyDateFormat.cjs",
      "default": "./lib/legacyDateFormat.js"
    },
    "./paths": {
      "source": "./src/paths.ts",
      "require": "./lib/paths.cjs",
      "default": "./lib/paths.js"
    }
  },
  "//": "the typesVersion config below is a workaround for TypeScript's lack of support for package exports",
  "typesVersions": {
    "*": {
      "_internal": [
        "./lib/dts/_internal.d.ts"
      ],
      "content": [
        "./lib/dts/content/index.d.ts"
      ],
      "legacyDateFormat": [
        "./lib/dts/legacyDateFormat.d.ts"
      ],
      "paths": [
        "./lib/dts/paths.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "../../../bin/pkg-utils bundle --target node --tsconfig tsconfig.lib.json",
    "clean": "rimraf lib",
    "prebuild": "yarn clean",
    "test": "jest",
    "watch": "../../../bin/pkg-utils bundle --target node --tsconfig tsconfig.lib.json --watch"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanity-io/sanity.git",
    "directory": "packages/@sanity/util"
  },
  "keywords": [
    "sanity",
    "cms",
    "headless",
    "realtime",
    "content",
    "util"
  ],
  "author": "Sanity.io <hello@sanity.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sanity-io/sanity/issues"
  },
  "homepage": "https://www.sanity.io/",
  "dependencies": {
    "@sanity/types": "2.29.5",
    "dotenv": "^10.0.0",
    "fs-extra": "^7.0.0",
    "get-random-values": "^1.2.2",
    "lodash": "^4.17.21",
    "moment": "^2.19.1",
    "resolve-from": "^5.0.0"
  },
  "devDependencies": {
    "@types/moment": "^2.13.0",
    "rimraf": "^3.0.2"
  }
}
