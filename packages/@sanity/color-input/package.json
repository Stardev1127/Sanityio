  },
  "devDependencies": {
    "@sanity/base": "2.30.0",
    "@sanity/types": "2.29.5",
    "prop-types": "^15.6.0",
    "react": "17.0.2",
    "styled-components": "^5.2.0"
  },
  "peerDependencies": {
    "@sanity/base": "^2.14",
