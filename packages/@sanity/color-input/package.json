  "version": "2.30.0",
  "description": "Color input",
  "source": "./src/ColorInput.jsx",
  "main": "./lib/ColorInput.cjs",
  "module": "./lib/ColorInput.js",
  "exports": {
    ".": {
      "source": "./src/ColorInput.jsx",
      "require": "./lib/ColorInput.cjs",
      "default": "./lib/ColorInput.js"
    },
    "./schemas/color": {
      "source": "./src/schemas/color.js",
      "require": "./lib/schemas/color.cjs",
      "default": "./lib/schemas/color.js"
    },
    "./schemas/hslaColor": {
      "source": "./src/schemas/hslaColor.js",
      "require": "./lib/schemas/hslaColor.cjs",
      "default": "./lib/schemas/hslaColor.js"
    },
    "./schemas/hsvaColor": {
      "source": "./src/schemas/hsvaColor.js",
      "require": "./lib/schemas/hsvaColor.cjs",
      "default": "./lib/schemas/hsvaColor.js"
    },
    "./schemas/rgbaColor": {
      "source": "./src/schemas/rgbaColor.js",
      "require": "./lib/schemas/rgbaColor.cjs",
      "default": "./lib/schemas/rgbaColor.js"
    }
  },
  "types": "lib/dts/src/ColorInput.d.ts",
  "author": "Sanity.io <hello@sanity.io>",
  "license": "MIT",
  "scripts": {
    "build": "../../../bin/pkg-utils bundle",
    "clean": "rimraf lib",
    "exports:check": "../../../bin/pkg-utils exports-check",
    "exports:clean": "../../../bin/pkg-utils exports-clean",
    "exports:generate": "../../../bin/pkg-utils exports-generate",
    "postbuild": "run-s exports:generate",
    "watch": "../../../bin/pkg-utils bundle --watch"
  },
  "keywords": [
    "sanity",
    "cms",
    "headless",
    "realtime",
    "content",
    "color-input",
