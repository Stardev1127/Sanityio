{
  "name": "@sanity/base",
  "version": "2.30.0",
  "description": "Sanity plugin containing the base components and roles for a Sanity configuration",
  "source": "./src/_exports/index.ts",
  "main": "./lib/cjs/_exports/index.js",
  "module": "./lib/esm/_exports/index.js",
  "exports": {
    ".": {
      "source": "./src/_exports/index.ts",
      "require": "./lib/cjs/_exports/index.js",
      "default": "./lib/esm/_exports/index.js"
    },
    "./_unstable": {
      "source": "./src/_exports/_unstable.ts",
      "require": "./lib/cjs/_exports/_unstable.js",
      "default": "./lib/esm/_exports/_unstable.js"
    },
    "./desk": {
      "source": "./src/_exports/desk.ts",
      "require": "./lib/cjs/_exports/desk.js",
      "default": "./lib/esm/_exports/desk.js"
    },
    "./form": {
      "source": "./src/_exports/form.ts",
      "require": "./lib/cjs/_exports/form.js",
      "default": "./lib/esm/_exports/form.js"
    }
  },
  "types": "./lib/dts/src/_exports/index.d.ts",
  "//": "the typesVersion config below is a workaround for TypeScript's lack of support for package exports",
  "typesVersions": {
    "*": {
      "_unstable": [
        "./lib/dts/src/_exports/_unstable.d.ts"
      ],
      "desk": [
        "./lib/dts/src/_exports/desk.d.ts"
      ],
      "form": [
        "./lib/dts/src/_exports/form.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "../../../bin/pkg-utils transpile --tsconfig tsconfig.lib.json",
    "clean": "rimraf lib",
    "coverage": "jest --coverage",
    "exports:check": "../../../bin/pkg-utils exports-check",
    "exports:clean": "../../../bin/pkg-utils exports-clean",
    "exports:generate": "../../../bin/pkg-utils exports-generate",
    "postbuild": "run-s exports:generate",
    "prepublishOnly": "node -r esbuild-register scripts/writeRequiredUIVersion.ts",
    "test": "jest",
    "watch": "../../../bin/pkg-utils transpile --tsconfig tsconfig.lib.json --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanity-io/sanity.git",
    "directory": "packages/@sanity/base"
  },
  "keywords": [
    "sanity",
    "cms",
    "headless",
    "realtime",
