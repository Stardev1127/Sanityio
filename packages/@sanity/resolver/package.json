{
  "name": "@sanity/resolver",
  "version": "0.0.1",
  "description": "Resolves parts and plugins from a Sanity configuration",
  "main": "lib/resolver.js",
  "scripts": {
    "clean": "rimraf lib && rimraf coverage",
    "compile": "npm run clean && babel -d lib/ src/",
    "coverage": "nyc --report=lcov --report=text _mocha -- test/**/*.test.js",
    "postpublish": "npm run clean",
    "posttest": "eslint .",
    "prepublish": "in-publish && npm run compile || not-in-publish",
    "test": "mocha test/**/*.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanity-io/resolver.git"
  },
  "keywords": [
    "sanity",
    "resolve"
  ],
  "author": "Sanity.io <hello@sanity.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sanity-io/resolver/issues"
  },
  "homepage": "https://github.com/sanity-io/resolver#readme",
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-eslint": "^6.0.0",
    "babel-preset-es2015": "^6.6.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.2.0",
    "eslint": "^3.3.0",
    "eslint-config-sanity": "^1.0.6",
    "mocha": "^2.4.5",
    "mock-fs": "^3.11.0",
    "nyc": "^8.3.0",
    "proxyquire": "^1.7.4",
    "rimraf": "^2.5.2"
  },
  "dependencies": {
    "@sanity/generate-help-url": "^0.0.2",
    "fs-promise": "^0.5.0",
    "in-publish": "^2.0.0",
    "lodash.uniq": "^4.5.0",
    "path-exists": "^3.0.0",
    "promise-props-recursive": "^1.0.0"
  }
}
