{
  "name": "@sanity/mutator",
  "version": "0.0.0",
  "description": "A set of models to make it easier to utilize the powerful real time collaborative features of Sanity",
  "main": "lib/index.js",
  "scripts": {
    "test": "tap test/*.test.js --node-arg --require --node-arg ./.babel-init.js",
    "test:lib:compile": "babel . -q --ignore node_modules,demo-server,perf --out-dir test-compiled",
    "test:lib": "npm run test:lib:compile && cd test-compiled && tap test/**.test.js && cd - && npm run test:lib:clean",
    "test:lib:clean": "rimraf test-compiled",
    "compile": "npm run clean && babel --out-dir lib/ src/",
    "compile:watch": "babel --watch --out-dir lib/ src/",
    "benchmark": "npm run compile && node --prof --logfile=benchmarks.log perf/benchmark.js",
    "prepublish": "npm run compile",
    "postpublish": "npm run clean",
    "clean": "rimraf lib"
  },
  "keywords": [],
  "author": "Sanity.io <hello@sanity.io>",
  "license": "MIT",
  "dependencies": {
    "diff-match-patch": "^1.0.0",
    "immutable": "^3.8.1",
    "lodash": "^4.15.0"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-eslint": "^7.0.0",
    "babel-plugin-lodash": "^3.2.9",
    "babel-plugin-syntax-class-properties": "^6.13.0",
    "babel-plugin-syntax-flow": "^6.13.0",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.16.0",
    "babel-plugin-transform-flow-strip-types": "^6.14.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-register": "^6.14.0",
    "babelify": "^7.3.0",
    "eslint": "^3.4.0",
    "eslint-config-bengler": "^2.0.0",
    "eslint-plugin-flowtype-errors": "^1.4.1",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-react": "^6.2.0",
    "tap": "^7.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanity-io/mutator.git"
  },
  "bugs": {
    "url": "https://github.com/sanity-io/mutator/issues"
  },
  "homepage": "https://github.com/sanity-io/mutator#readme"
}
