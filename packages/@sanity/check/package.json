{
  "name": "@sanity/check",
  "version": "1.0.4",
  "description": "Sanity check Sanity plugins and projects",
  "main": "lib/sanityCheck.js",
  "bin": {
    "sanity-check": "./lib/bin.js"
  },
  "author": "Espen Hovlandsdal <espen@hovlandsdal.com>",
  "license": "MIT",
  "scripts": {
    "compile": "babel src --copy-files --out-dir lib",
    "compile:watch": "babel --watch --copy-files -d lib/ src/",
    "prepublish": "in-publish && npm run compile && ./lib/bin.js || not-in-publish",
    "postpublish": "rimraf lib",
    "test": "eslint ."
  },
  "keywords": [
    "sanity",
    "sanity-plugin"
  ],
  "dependencies": {
    "@sanity/generate-help-url": "^1.0.0",
    "@sanity/resolver": "^3.0.3",
    "chalk": "^1.1.3",
    "fs-promise": "^0.5.0",
    "in-publish": "^2.0.0",
    "promise-props-recursive": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-eslint": "^6.0.4",
    "babel-preset-es2015": "^6.9.0",
    "eslint": "^3.3.0",
    "eslint-config-sanity": "^1.1.1",
    "rimraf": "^2.5.2"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:sanity-io/check.git"
  }
}
